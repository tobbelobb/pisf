// Create all bush nodes
MERGE (reprap:Reprap {name : "Reprap"})
  ON CREATE SET reprap.created=timestamp()
  ON MATCH SET reprap.accessed=timestamp()
MERGE (printed_object:Category {name : "Printed Object"})
  ON CREATE SET printed_object:Bush, printed_object.created=timestamp()
  ON MATCH SET printed_object.accessed=timestamp()
MERGE (theory:Category {name : "Theory"})
  ON CREATE SET theory:Bush, theory.created=timestamp()
  ON MATCH SET theory.accessed=timestamp()
MERGE (software:Category {name : "Software"})
